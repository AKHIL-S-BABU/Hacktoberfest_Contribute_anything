#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t,n,x,y,k;
	cin >> t;
	for(int i = 0; i < t; i++){
	    cin >> n >> k >> x >> y;
	    int f = 1;
	    int p = x;
	    while(f){
	        x = (x+k)%n;
	        if(x == y){
	            cout << "YES" << endl;
	            break;
	        }
	        if(x == p){
	            cout << "NO" << endl;
	            break;
	        }
	    }
	}
	return 0;
}

